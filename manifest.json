{
  "manifest_version": 2,
  "name": "Qiscus Hardcoded",
  "description": "Qiscus chrome extension - hardcoded",
  "version": "1.0",
  "permissions": [
    "https://qisc.us/api/v1/mobile/",
    "https://qiscus-staging.herokuapp.com/",
    "notifications",
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://js.pusher.com https://cdnjs.cloudflare.com; object-src 'self'",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "login.html"
  }
}
